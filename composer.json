{
  "name": "icerainliu/php-helper",
  "type": "library",
  "version": "v1.0.0",
  "keywords": [
    "php",
    "helper"
  ],
  "description": "PHP的帮助类和方法",
  "license": "Apache-2.0",
  "require": {
    "php": ">7.4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5"
  },
  "autoload": {
    "psr-4": {
      "PhpHelper\\Stdlib\\": "src/"
    },
    "files": [
      "src/Helper/Functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "SwoftTest\\Stdlib\\Unit\\": "test/unit/",
      "SwoftTest\\Stdlib\\Testing\\": "test/testing/"
    }
  },
  "repositories": {
  },
  "scripts": {
    "test": "./vendor/bin/phpunit -c phpunit.xml"
  }
}
